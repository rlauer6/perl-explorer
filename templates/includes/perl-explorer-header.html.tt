<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

  [% FOREACH file IN css -%]
  <link rel="stylesheet" type="text/css" href="[% file %]" />
  [% END %]
  
  [% FOREACH file IN js -%]
  <script src="[% file %]" type="text/javascript"></script>
  [% END %]

  [% IF title %]<title>[% title %]</title>[% END %]
  
</head>
